pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building job'
            }
        }
        stage('approval') {
            steps {
                input message: 'deploying to prod ? ready ?', ok: 'yes , i am sure i want to deploy'
            }
        }
        stage('deploy') {
            steps {
                echo 'stage deploy'
            }
        }
    }
}

